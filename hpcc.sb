#!/bin/sh

#SBATCH -A ecode

## Email settings
#SBATCH --mail-type=FAIL
#SBATCH --mail-user=leithers@msu.edu

## Job name settings
#SBATCH --job-name=graph_evolution
#SBATCH -o ga_%A.out

## Time requirement in format "days-hours:minutes"
#SBATCH --time=0-1:00

## Memory requirement in megabytes. You might need to make this bigger.
#SBATCH --mem-per-cpu=1gb

#SBATCH --array=0-9

cd /mnt/scratch/leithers/graph-evolution

#make directory with config name
mkdir $1
cd $1

#make directory for each run within the config
mkdir ${SLURM_ARRAY_TASK_ID}
cd ${SLURM_ARRAY_TASK_ID}

# Get scripts
cp /mnt/home/leithers/graph_evolution/graph-evolution/${1}.json .
cp /mnt/home/leithers/graph_evolution/graph-evolution/hpcc_main.py .
cp /mnt/home/leithers/graph_evolution/graph-evolution/ga.py .
cp /mnt/home/leithers/graph_evolution/graph-evolution/eval_functions.py .
cp /mnt/home/leithers/graph_evolution/graph-evolution/organism.py .

# Run script
python3 hpcc_main.py ${1}.json > outfile.txt